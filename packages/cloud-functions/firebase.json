{
  "functions": {
    "source": "."
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/v2/dialogflow",
        "function": "dialogflow"
      },
      {
        "source": "/api/v1/glucoseStatus",
        "function": "glucoseStatus"
      },
      {
        "source": "/api/v1/validateUrl",
        "function": "validateUrl"
      }
    ]
  }
}
